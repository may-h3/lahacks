{
    "manifest_version": 3,
    "name": "la hacks project",
    "version": "1.0",
    "description": "site screen time",
    "action": {
    "default_popup": "popup.html",
    "default_icon": {
          "32": "apidlogo.png" }
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "activeTab", 
        "browsingData", 
        "history",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    }
}